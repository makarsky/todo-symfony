{% extends "base.html.twig" %}

{% block body %}
    <div class="container">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            {{ form_start(form) }}
                {{ form_row(form.username) }}
                {{ form_row(form.email) }}
                {{ form_row(form.password) }}
            {{ form_end(form) }}
        </div>
        <div class="col-md-3"></div>
    </div>
{% endblock %}